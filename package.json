{
  "name": "base-sample",
  "displayName": "base-sample",
  "description": "Base sample for other samples at Microsoft/vscode-extension-samples",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateAll",
    "onCommand:extension.generateState"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateAll",
        "title": "Generate All"
      },
      {
        "command": "extension.generateState",
        "title": "Generate State"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.generateAll",
          "group": "generateGroup"
        },
        {
          "command": "extension.generateState",
          "group": "generateGroup"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.32.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^3.9.4"
  }
}
